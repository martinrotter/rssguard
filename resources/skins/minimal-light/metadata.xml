<?xml version="1.0" encoding="UTF-8"?>
<skin version="6.6.6" base="minimal-base">
  <author>
    <name>martinrotter</name>
  </author>
  <description>Simple and good-looking light skin.</description>

  <palette>
    <color key="FgInteresting">#2A7BD4</color>
    <color key="FgSelectedInteresting">#155A9E</color>
    <color key="FgNewMessages">#3b8a5a</color>
    <color key="FgSelectedNewMessages">#1f7a45</color>
    <color key="FgError">#e74c3c</color>
    <color key="FgSelectedError">#c0392b</color>
    <color key="FgDisabledFeed">#7f8c8d</color>
    <color key="Allright">#229954</color>
  </palette>

  <forced-styles></forced-styles>
  <forced-skin-colors></forced-skin-colors>
  
  <style-palette>
    <group id="All">
      <color role="WindowText">#000000</color>
      <color role="ButtonText">#000000</color>
      <color role="Window">#efefef</color>
  
      <color role="Light">#ffffff</color>
      <color role="Midlight">#f4f4f4</color>
      <color role="Mid">#d2d2d2</color>
      <color role="Dark">#a0a0a0</color>
  
      <color role="Text">#000000</color>
      <color role="Base">#ffffff</color>
  
      <color role="Button">#efefef</color>
      <color role="Shadow">#646464</color>
      <color role="HighlightedText">#ffffff</color>
      <color role="PlaceholderText">#80000000</color>
      <color role="Highlight">#73a5b6</color>
    </group>
  
    <group id="Disabled">
      <color role="Text">#bebebe</color>
      <color role="WindowText">#bebebe</color>
      <color role="ButtonText">#bebebe</color>
      <color role="Dark">#bebebe</color>
      <color role="Shadow">#828282</color>
      <color role="Highlight">#919191</color>
    </group>
  </style-palette>
</skin>
